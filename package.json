{
	"name": "practice-application",
	"private": true,
	"version": "0.1.0",
	"type": "commonjs",
	"author": "Yagnik Vadi",
	"description": "Starter kit for creating apps with React and Typescript application",
	"keywords": [
		"TypeScript",
		"React",
		"Web Development"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/CreativeThinker2023/counter-application",
		"directory": "src"
	},
	"bugs": {
		"url": "https://github.com/CreativeThinker2023/counter-application/issues"
	},
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"lint": "eslint src/**/*.{js,jsx,ts,tsx,json}",
		"lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx,json}'",
		"prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
		"pre:commit": "npm run prettier && npm run lint:fix",
		"preview": "vite preview"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": "yarn run lint:fix",
		"**/*.{js,jsx,tsx,ts,less,md,json}": [
		  	"prettier --write"
		]
	},
	"engines": {
		"node": "18.x",
		"npm": "9.x",
		"yarn": "1.22.x"
	},
	"dependencies": {
		"@reduxjs/toolkit": "^1.9.5",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-redux": "^8.1.0",
		"react-router-dom": "^6.13.0",
		"redux-logger": "^3.0.6",
		"redux-saga": "^1.2.3",
		"uuid": "^9.0.0"
	},
	"devDependencies": {
		"@types/react": "^18.0.37",
		"@types/react-dom": "^18.0.11",
		"@types/react-redux": "^7.1.25",
		"@types/react-router-dom": "^5.3.3",
		"@types/redux-saga": "^0.10.5",
		"@typescript-eslint/eslint-plugin": "^5.59.0",
		"@typescript-eslint/parser": "^5.59.0",
		"@vitejs/plugin-react": "^4.0.0",
		"eslint": "^8.42.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-config-standard-with-typescript": "^35.0.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.3.4",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"prettier": "^2.8.8",
		"prettier-eslint": "^15.0.1",
		"typescript": "^5.0.2",
		"vite": "^4.3.9"
	}
}
